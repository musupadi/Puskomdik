                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:src="@drawable/biaya_akademik"/>
                            <TextView
                                android:fontFamily="@font/roboto"
                                android:textStyle="bold"
                                android:textColor="@color/colorBlack"
                                android:gravity="center|top"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="2"
                                android:textSize="@dimen/text_smoll"
                                android:text="Biaya\nAkademik"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:layout_margin="5dp"
                            android:id="@+id/linearKoperasi">
                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:src="@drawable/koperasi"/>
                            <TextView
                                android:fontFamily="@font/roboto"
                                android:textStyle="bold"
                                android:textColor="@color/colorBlack"
                                android:gravity="center|top"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="2"
                                android:textSize="@dimen/text_smoll"
                                android:text="Koperasi"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:visibility="invisible"
                            android:layout_margin="5dp">
                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:src="@drawable/lihat_semua"/>
                            <TextView
                                android:fontFamily="@font/roboto"
                                android:textStyle="bold"
                                android:textColor="@color/colorBlack"
                                android:gravity="center"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="2"
                                android:textSize="@dimen/text_medium"
                                android:text="Lihat\nSemua"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </HorizontalScrollView>
        <LinearLayout
            android:id="@+id/linearKembali"
            android:layout_marginRight="30dp"
            android:layout_marginBottom="8dp"
            android:layout_marginTop="8dp"
            android:layout_gravity="right"
            android:gravity="right"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_rounded_primary">

            <Button
                android:id="@+id/btnKembali"
                android:layout_width="match_parent"
                android:layout_height="28dp"
                android:layout_margin="2dp"
                android:background="@drawable/btn_rounded"
                android:text="Kembali"
                android:fontFamily="@font/roboto"
                android:textColor="@color/colorPrimary"
                android:textSize="@dimen/text_xtra_large" />
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>